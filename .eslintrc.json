{
  "extends": [
    "airbnb",
    "plugin:flowtype/recommended"
  ],
  "plugins": [
    "flowtype",
    "compat"
  ],
  "env": {
    "browser": true,
    "jest": true
  },
  "rules": {
    "quotes": "off",
    "linebreak-style":"off",
    "no-restricted-imports":"off",
    "comma-dangle": "off",
    "arrow-parens": "off"
  },
  "settings": {
    "polyfills": ["fetch"]
  }
}
